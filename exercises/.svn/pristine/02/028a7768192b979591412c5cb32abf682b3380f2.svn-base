body
  position: absolute
  top: 0
  bottom: 0
  width: 100%
  font-size: 0.14rem
  display: flex
  flex-direction: column
  background: #f6f6f6
header
  padding: 0.09rem
  display: -webkit-flex
  justify-content: space-between
  align-items: center
  background: #ffffff

section
  overflow-y: auto
  flex: 1
  .logon
    color: #ffffff
    display: flex
    flex-direction: column
    justify-content: center
    background: #846c7f
    .top
      padding: 0.14rem 1.18rem
      text-align: center
      button
        border-width: 0
        background: #ffffff
        width: 40%
        min-width: 0.83rem
        height: 0.28rem
        border-radius: 0.03rem
        outline: none
        color: #fa529f
    .middle
      text-align: center
      padding-left: 0.5rem
      padding-right: 0.5rem
      font-size: 0.12rem
      button
        border-width: 0
        color: #dbc9cc
        background: rgba(255,255,255,0.3)
        width: 0.62rem
        height: 0.19rem
        border-radius: 0.08rem
        text-align: center
        line-height: 0.19rem
    .last
      margin-top: 0.19rem
      display: flex
      background: rgba(255,255,255,0.1)
      p
        padding-top: 0.03rem
        padding-bottom: 0.03rem
        flex: 1
        display: flex
        flex-direction: column
        justify-content: center
        align-items: center
        span
          margin-top: 0.03rem
      .left
        border-right: 0.01rem solid #dbc9cc
  .over
    margin-top: 0.03rem
    display: flex
    justify-content: space-between
    padding: 0.11rem 0.09rem
    background: #ffffff
    margin-bottom: 0.02rem
    p
      color: #a0a0a0
  .list
    background: #ffffff
    display: flex
    margin-bottom: 0.04rem
    li
      padding-top: 0.1rem
      padding-bottom: 0.08rem
      display: flex
      flex-direction: column
      justify-content: center
      align-items: center
      flex: 1
      span
        margin-top: 0.07rem
  .main
    display: flex
    padding-bottom: 0.09rem
    background: #ffffff
    flex-wrap: wrap
    margin-bottom: 0.03rem
    li
      font-size: 0.10rem
      margin-top: 0.14rem
      width: 25%
      display: flex
      justify-content: center
      align-items: center
      flex-direction: column
      span
        margin-top: 0.06rem
      p
        border-radius: 0.03rem
        width: 0.3rem
        height: 0.3rem
        text-align: center
        line-height: 0.3rem
        i
          color: #ffffff
  .phone
    display: flex
    justify-content: space-between
    padding: 0.1rem 0.08rem
    background: #ffffff
    margin-bottom: 0.02rem
    i
      color: #a0a0a0
  .number
    margin-bottom: 0.04rem
    display: flex
    justify-content: space-between
    padding: 0.1rem 0.08rem
    background: #ffffff
    p
      color: #fa66aa
    i
      margin-left: 0.04rem
      color: #a0a0a0
  .banner
    margin-bottom: 0.02rem
    .swiper-slide
      height: 1.05rem
      img
        width: 100%
        height: 100%
footer
  background: #fff
  ul
    display: flex
    border-top: 0.01rem solid #CCCCCC
    li
      flex: 1
      padding-top: 0.06rem
      padding-bottom: 0.06rem
      a
        display: flex
        flex-direction: column
        align-items: center
        justify-content: center
      .begin
        color: #fa529f
        i
          font-size: 0.16rem
        span
          margin-top: 0.04rem
          font-size: 0.12rem